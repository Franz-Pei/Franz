      <rect width="64" height="64" fill="#8b6d5c"/>
      
      <!-- Darker soil specs -->
      <g fill="#7a5c4d" opacity="0.7">
        <circle cx="10" cy="10" r="2" />
        <circle cx="30" cy="5" r="3" />
        <circle cx="50" cy="12" r="2.5" />
        <circle cx="15" cy="25" r="2" />
        <circle cx="38" cy="28" r="3" />
        <circle cx="55" cy="30" r="2" />
        <circle cx="8" cy="45" r="2.5" />
        <circle cx="25" cy="50" r="2" />
        <circle cx="45" cy="48" r="3" />
        <circle cx="60" cy="55" r="2.5" />
      </g>
      
      <!-- Lighter soil specs -->
      <g fill="#97796a" opacity="0.5">
        <circle cx="5" cy="15" r="1.5" />
        <circle cx="22" cy="8" r="2" />
        <circle cx="40" cy="15" r="1.5" />
        <circle cx="58" cy="20" r="2" />
        <circle cx="12" cy="35" r="1.5" />
        <circle cx="30" cy="38" r="2" />
        <circle cx="48" cy="35" r="1.5" />
