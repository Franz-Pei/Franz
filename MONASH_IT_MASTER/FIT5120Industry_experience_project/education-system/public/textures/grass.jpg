        <path d="M250,135 C255,120 260,115 265,105 C264,125 261,130 259,140 Z" />
        <path d="M320,130 C325,115 330,110 333,100 C331,120 329,125 327,135 Z" />
        <path d="M380,125 C385,110 390,105 393,100 C391,120 389,125 386,135 Z" />
