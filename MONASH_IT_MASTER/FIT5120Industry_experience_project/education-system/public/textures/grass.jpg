        <path d="M100,230 C105,215 110,210 112,205 C113,225 111,228 109,235 Z" />
        <path d="M180,225 C185,210 188,205 192,200 C190,220 188,225 185,235 Z" />
        <path d="M240,235 C245,220 250,215 255,205 C254,225 251,230 249,240 Z" />
        <path d="M310,230 C315,215 320,210 323,200 C321,220 319,225 317,235 Z" />
        <path d="M390,225 C395,210 400,205 403,200 C401,220 399,225 396,235 Z" />
