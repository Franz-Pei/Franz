<svg width="512" height="512" viewBox="0 0 512 512" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <!-- Base grass pattern -->
    <pattern id="grassPattern" patternUnits="userSpaceOnUse" width="64" height="64">
      <rect width="64" height="64" fill="#7ec850"/>
      
      <!-- Darker grass details -->
      <g fill="#6db649" opacity="0.7">
        <path d="M0,0 L12,8 L0,16 M16,16 L28,24 L16,32 M32,0 L44,8 L32,16 M48,16 L60,24 L48,32"/>
