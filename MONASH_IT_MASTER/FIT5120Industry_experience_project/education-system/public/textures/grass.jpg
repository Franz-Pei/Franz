        <ellipse cx="380" cy="120" rx="50" ry="40" fill="url(#soilPattern)"/>
        <ellipse cx="200" cy="350" rx="45" ry="35" fill="url(#soilPattern)"/>
        <ellipse cx="420" cy="400" rx="30" ry="25" fill="url(#soilPattern)"/>
      </g>
      
      <!-- Individual grass blades -->
      <g fill="#8ed160">
        <!-- First row -->
        <path d="M30,40 C35,25 40,20 45,10 C45,25 43,30 40,40 Z" />
        <path d="M80,30 C85,15 90,10 92,5 C93,25 91,28 89,35 Z" />
        <path d="M150,25 C155,10 158,5 162,0 C160,20 158,25 155,35 Z" />
