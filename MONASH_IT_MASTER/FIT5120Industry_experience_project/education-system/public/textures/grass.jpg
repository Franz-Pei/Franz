        <path d="M120,430 C125,415 130,410 132,405 C133,425 131,428 129,435 Z" />
        <path d="M185,425 C190,410 193,405 197,400 C195,420 193,425 190,435 Z" />
        <path d="M270,435 C275,420 280,415 285,405 C284,425 281,430 279,440 Z" />
